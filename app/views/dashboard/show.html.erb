<div class="container center">
    <h5>Lifetime Distance: <%= @aggregate_data.aggregate_distance %> miles</h5>
    <h5>Lifetime Calories: <%= @aggregate_data.aggregate_calories %> calories</h5>
    <h5>Lifetime Steps: <%= @aggregate_data.aggregate_steps %> steps</h5>
    <h5>Lifetime Sessions: <%= @aggregate_data.aggregate_sessions %> sessions</h5>
    <h2 class="center"> <%= current_user.display_name %></h2>
    <h2 class="center"> <%= current_user.email %></h2>
    <%= link_to "Logout", "/logout", method: :delete, class: "btn btn-lg btn-default btn-home" %>
    <%= link_to "View Workouts", workouts_path %>
    <%#= link_to "View Workouts", "/workouts", class: "btn btn-lg btn-default btn-home" %>
  </div>
</div>
